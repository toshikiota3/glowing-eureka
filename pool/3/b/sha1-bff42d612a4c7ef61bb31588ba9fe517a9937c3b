[CLOCK_DATE_FORMAT]
eng "%A, %B %e, %Y"
fre "%Y年%b%e日%a現在"
ita "%A, %B %e, %Y"
ger "%A, %e. %B %Y"
spa "%A, %e de %B, %Y"
dut "%A, %e. %B %Y"
swe "%y/%m/%d"

[CLOCK_TIME_FORMAT]
eng "%l:%M"
fre "%k:%M"
ita "%l:%M"
ger "%k:%M"
spa "%k:%M"
dut "%k:%M"
swe "%H:%M"

[CLOCK_TIME_AM]
eng "AM"
fre "AM"
ita "AM"
ger "AM"
spa "AM"
dut "AM"
swe "AM"

[CLOCK_TIME_PM]
eng "PM"
fre "PM"
ita "PM"
ger "PM"
spa "PM"
